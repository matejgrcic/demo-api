{
    "name": "demo-api",
    "version": "1.0.0",
    "description": "Simple demo api",
    "scripts": {
        "build": "tsc",
        "serve": "node build/server.js",
        "start": "npm run serve",
        "watch": "tsc -w",
        "lint": "eslint src/ --ext .ts",
        "dev": "npm run build && npm run start-debug",
        "serve-debug": "nodemon --inspect build/server.js",
        "start-debug": "concurrently \"npm run watch\" \"npm run serve-debug\""
    },
    "dependencies": {
        "axios": "^0.19.0",
        "body-parser": "^1.19.0",
        "concurrently": "^5.0.0",
        "express": "^4.17.1",
        "express-validator": "^6.2.0",
        "mongoose": "^5.7.5"
    },
    "author": "Matej Grcic",
    "license": "MIT",
    "devDependencies": {
        "@types/express": "^4.17.1",
        "@types/mongoose": "^5.5.21",
        "@typescript-eslint/eslint-plugin": "^2.4.0",
        "@typescript-eslint/parser": "^2.4.0",
        "concurrently": "^5.0.0",
        "eslint": "^6.5.1",
        "eslint-config-airbnb-base": "^14.0.0",
        "eslint-config-standard": "^14.1.0",
        "eslint-plugin-import": "^2.18.2",
        "eslint-plugin-node": "^10.0.0",
        "eslint-plugin-promise": "^4.2.1",
        "eslint-plugin-standard": "^4.0.1",
        "nodemon": "^1.19.4",
        "typescript": "^3.6.4"
    }
}
